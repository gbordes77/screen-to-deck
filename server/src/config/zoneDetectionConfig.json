  "preprocessing": {
    "enhanceContrast": {
      "enabled": true,
      "alpha": 1.5,
      "beta": 0
    },
    "denoise": {
      "enabled": true,
      "h": 10,
      "templateWindowSize": 7,
      "searchWindowSize": 21
    },
    "sharpen": {
      "enabled": true,
      "kernel": [
        [0, -1, 0],
        [-1, 5, -1],
        [0, -1, 0]
      ]
    },
    "resize": {
      "enabled": false,
      "targetWidth": 1920,
      "maintainAspectRatio": true
    },
    "colorCorrection": {
      "enabled": true,
      "gammaCorrection": 1.2
    }
  },
  "detection": {
    "confidence_threshold": 0.7,
    "min_text_height": 10,
    "max_text_height": 100,
    "text_padding": 5,
    "batch_size": 5,
    "parallel_processing": true,
    "cache_results": true,
    "fallback_methods": ["tesseract", "template_matching"]
  },
  "landCorrection": {
    "enabled": true,
    "patterns": {
      "plains": ["Plain", "Plai", "Pla ins", "P lains"],
      "island": ["Islan", "Is land", "lsland", "I sland"],
      "swamp": ["Swam", "S wamp", "Sw amp", "5wamp"],
      "mountain": ["Mountai", "M ountain", "Mount ain", "Mounta in"],
      "forest": ["Fore st", "F orest", "For est", "Fores t"]
    },
    "confidence_boost": 0.15
  },
  "validation": {
    "scryfall_api": true,
    "fuzzy_matching": true,
    "min_similarity": 0.75,
    "cache_duration": 86400,
    "retry_attempts": 3,
    "timeout": 5000
  },
  "export": {
    "formats": ["mtga", "moxfield", "archidekt", "tappedout", "json", "csv"],
    "default_format": "mtga",
    "include_metadata": true,
    "compress_output": false
  }
}