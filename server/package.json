{
  "name": "mtg-deck-converter-server",
  "version": "2.0.1",
  "description": "Backend server for MTG deck converter with OCR capabilities",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:real": "jest real-images.test.ts",
    "test:generate-images": "node tests/test-images/create-mtg-test-images.js",
    "test:e2e": "npm run test:generate-images && npm run test:real",
    "test:integration": "jest tests/integration/ocr-e2e.test.ts --testTimeout=300000",
    "test:integration:real": "OPENAI_API_KEY=$OPENAI_API_KEY ts-node tests/integration/ocr-e2e.test.ts",
    "test:integration:runner": "OPENAI_API_KEY=$OPENAI_API_KEY ts-node tests/integration/test-runner.ts",
    "test:all": "npm run test && npm run test:integration",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@supabase/supabase-js": "^2.43.4",
    "@types/node": "^20.14.2",
    "axios": "^1.6.2",
    "bullmq": "^5.7.14",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "joi": "^17.11.0",
    "lru-cache": "^11.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.52.1",
    "pino": "^9.3.2",
    "pino-http": "^10.3.0",
    "prom-client": "^15.1.3",
    "redis": "^4.6.14",
    "sharp": "^0.33.4",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "yamljs": "^0.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "mtg",
    "magic-the-gathering",
    "ocr",
    "api",
    "typescript",
    "express"
  ],
  "author": "MTG Deck Converter",
  "license": "MIT"
}
